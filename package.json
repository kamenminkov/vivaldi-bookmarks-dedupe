	"devDependencies": {
		"@types/lodash": "4.14.123",
		"@types/node": "12.12.8",
		"sanitize-filename": "1.6.3",
		"ts-node": "8.5.2",
		"typescript": "3.7.2"
	},
	"dependencies": {
		"lodash": "4.17.15"
	}
}